@model IEnumerable<onelv_parser.Models.MobilePhone>

@{
    ViewBag.Title = "Search";
    
}


@Html.Action("Index")
<br />


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Store)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
           @String.Format("{0:c2}", @Html.DisplayFor(modelItem => item.Price))
        </td>
        <td>
          <a href="@Html.DisplayFor(modelItem => item.Url)">visit</a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Store)
        </td>
     
    </tr>
}

</table>
